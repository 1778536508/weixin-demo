@import "/template/welfare-item/welfare-item-template.wxss";
@import "/template/progress-item/progress-item-template.wxss";
.welfare_list {
  width: 750rpx;
  padding-bottom: 50rpx;
}
.minToptab {
  position: fixed;
  right: 50rpx;
  bottom: 200rpx;
  width: 80rpx;
  height: 60rpx;
  font-size: 50rpx;
}
.minToptab icon{
  position: absolute;
  left: 0;
  top: 10rpx;
  right: 0;
  bottom: 0;
  margin: auto;
  opacity: .5;
}
.refresh-block {
  text-align: center;
  padding-bottom: 50rpx;
}
.roof_placement {
  position: fixed;
  right: 60rpx;
  bottom: 300rpx;
  font-size: 80rpx;
  color: rgba(248,248,248,.5);
}
.blank_page {
  position: fixed;
  left: 0;
  top: 2px;
  width: 100%;
  height: 100%;
  background: #fff;
}